coeficiente <- function(x, y){
n <- sum(x)/mean(x)
soma <- sum(x*y)
numerador <- soma - n*mean(x)*mean(y)
x1 <- x*x
y1 <- y*y
denominador <- sqrt(sum(x1)-n*mean(x)*mean(x))*sqrt(sum(y1)-n*mean(y)*mean(y))
return (numerador/denominador)
}
